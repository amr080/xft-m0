<script src="https://cdn.jsdelivr.net/combine/npm/jquery@1.11,npm/midnight.js@1.1"></script>
<script>
$(document).ready(function() {
  // Check if the screen width is greater than 991px
  if(window.innerWidth > 991) {
    // Start midnightJS for larger screens
    $('.navbar_component').midnight();

    // Function to update button classes based on header classes
    function updateButtonClasses() {
      // Iterate over each midnightHeader
      $('.midnightHeader').each(function() {
        var $header = $(this);
        var $button = $header.find('.navbar_button-wrapper .w-button');

        // Check for specific combo classes and update button class accordingly
        if ($header.hasClass('grey') || $header.hasClass('white') || $header.hasClass('teal')) {
          $button.removeClass('is-secondary').addClass('is-black-outline');
        } else {
          $button.removeClass('is-black-outline').addClass('is-secondary');
        }
      });
    }

    // Initial update
    updateButtonClasses();
  }
});
</script><script>
const allFaqLinks = document.querySelectorAll(`[data="faq-links"]`);
for(const eachFaqLink of allFaqLinks){
const hrefValue = eachFaqLink.getAttribute("data-href");
eachFaqLink.href = `#` + hrefValue;

console.log(eachFaqLink)
}
